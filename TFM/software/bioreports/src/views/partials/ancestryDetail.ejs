<!-- views/partials/ancestryDetail.ejs -->
<div class="contentInfo">
    <h3><%= details.fileName %></h3>

    <table class="table table-striped">
        <thead style="color: #fff; background-color: #212529; border-color: #32383e;">
            <th scope="col">REGION</th>
            <th scope="col">REGION VALUE</th>
        </thead>
        <tbody>
            <% Object.keys(details.ancestry[0]).forEach(function(region) { %>
                <tr>
                    <td scope="col"><%= region %></td>
                    <td scope="col"><%= parseFloat(details.ancestry[0][region]).toFixed(6) %></td>
                </tr>
            <% }); %>    
        </tbody>
    </table>

</div>